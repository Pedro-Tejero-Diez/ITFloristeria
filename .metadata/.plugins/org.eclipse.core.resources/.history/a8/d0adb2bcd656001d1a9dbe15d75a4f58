package ejercicio1;

import java.util.ArrayList;

public class S01T02N01TEJERODIEZPEDROmain {

	public static void main(String[] args) {
		
		ArrayList<Producto> listado = new ArrayList<Producto>();
		
		Producto pr1 = new Producto("patatas", 100);
		Producto pr2 = new Producto("zanahorias", 50);
		Producto pr3 = new Producto("calabacin", 75);
		
		listado.add(pr1);
		listado.add(pr2);
		listado.add(pr3);
		
		Venta venta = new Venta(listado);
		
		
		for (int i=0; i<3; i++) {
		System.out.println(listado.get(i));
		}
		System.out.println("La venta total ha sido: "+venta.calcularTotal());
	}
	}

